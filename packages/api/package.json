{
  "name": "reaql-backend-api",
  "version": "0.0.1",
  "description": "nest js graphql",
  "author": "dan-exxx1st",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_PATH=./.. ts-node ./src/main.ts",
    "dev": "cross-env NODE_PATH=../ nodemon -L ./src/main.ts",
    "debug": "cross-env NODE_PATH=../ nodemon  ./src/main.ts --inspect",
    "graphql:generate": "ts-node generate-typings",
    "graphql:generate:watch": "cross-env IS_WATCH=true ts-node generate-typings",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "start:prod": "cross-env NODE_PATH=./dist node dist/api/src/main.js"
  },
  "dependencies": {
    "@apollo/federation": "^0.20.6",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.5.5",
    "@nestjs/microservices": "^7.5.2",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/typeorm": "^7.1.0",
    "apollo-server-express": "^2.19.0",
    "graphql": "^15.3.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^6.0.13",
    "pg": "^8.3.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "typeorm": "^0.2.25",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.3.2",
    "@types/express": "4.17.8",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "apollo-server-testing": "^2.19.2",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "nodemon": "^2.0.6",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  }
}
